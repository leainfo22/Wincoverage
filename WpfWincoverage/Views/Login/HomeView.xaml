<Page x:Class="WpfWincoverage.Login.HomeView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfWincoverage.Login"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="HomeView">

    <Grid>

        <Image HorizontalAlignment="Center" Height="33" Margin="0,120,0,0" VerticalAlignment="Top" Width="208" RenderTransformOrigin="1.5,0.5" Source="/logoGeyser.JPG">
            <Image.OpacityMask>
                <ImageBrush ImageSource="logoGeyser.JPG"/>
            </Image.OpacityMask>
        </Image>
        <Label x:Name="labelWelcome" HorizontalAlignment="Center" Margin="0,265,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="24"/>
        <Label x:Name="labelCompany" Content="Nombre empresa y numero de licencia" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</Page>
