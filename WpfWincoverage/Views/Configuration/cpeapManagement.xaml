        <Button Content="Remove" HorizontalAlignment="Center" Margin="0,339,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.348,-0.414" Width="64" Height="41"/>
        <Button Content="Import" HorizontalAlignment="Left" Margin="539,339,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.348,-0.414" Width="64" Height="41"/>
        <Button Content="Visualize" HorizontalAlignment="Left" Margin="632,339,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.348,-0.414" Width="64" Height="41"/>
        <Label x:Name="labelAP" Content="" Canvas.Left="679" Canvas.Top="167"/>
        <Label Content="de" Canvas.Left="626" Canvas.Top="167"/>
        <Button Content="Next" Canvas.Left="728" Canvas.Top="170" RenderTransformOrigin="0.038,-0.087"/>
        <Button Content="Before" Canvas.Left="526" Canvas.Top="170" RenderTransformOrigin="0.038,-0.087"/>
        <Label x:Name="labelCPE" Content="" Canvas.Left="679" Canvas.Top="245"/>
        <Label Content="de" Canvas.Left="626" Canvas.Top="245"/>
        <Button Content="Next" Canvas.Left="728" Canvas.Top="248" RenderTransformOrigin="0.038,-0.087"/>
        <Button Content="Before" Canvas.Left="526" Canvas.Top="248" RenderTransformOrigin="0.038,-0.087"/>
        <TextBox Canvas.Left="586" Canvas.Top="173" Text="1" TextWrapping="Wrap" Width="14" Height="17"/>
        <TextBox Canvas.Left="586" Canvas.Top="251" Text="1" TextWrapping="Wrap" Width="14" Height="17"/>

    </Canvas>
