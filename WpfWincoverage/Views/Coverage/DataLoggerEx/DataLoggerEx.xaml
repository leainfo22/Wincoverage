        <TextBox x:Name="textBox1_Copy3" Canvas.Left="496" Canvas.Top="266" Text="" TextWrapping="Wrap" Width="91"/>
        <Button x:Name="buttonEdit" Content="Find" RenderTransformOrigin="0.545,0.502" Height="41" Width="60"  Canvas.Left="217" Canvas.Top="311"/>
