<Window x:Name="MainWindowName" x:Class="WpfWincoverage.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfWincoverage"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Menu IsMainMenu="True" VerticalAlignment="Top">
            <MenuItem Header="_Login">
                <MenuItem Header="_Log in" Click="Login_Click"/>
                <MenuItem Header="_Log out" IsEnabled="False"/>
                <Separator />
                <MenuItem Header="_Exit" Click="Exit_Click"/>
            </MenuItem>
        </Menu>
        <Frame x:Name="Main" Margin="10,23,10,10" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
